{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000",
        "Protocols": "Http1AndHttp2"
      },
      "gRPC": {
        "Url": "http://localhost:5002",
        "Protocols": "Http2"
      }
    }
  },
  "WebpEncoderOptions": {
    "Quality": 80,
    "SkipMetadata": true,
    "FileFormat": "Lossy",
    "Method": "Level3",
    "UseAlphaCompression": true,
    "EntropyPasses": 1,
    "SpatialNoiseShaping": 0,
    "FilterStrength": 20,
    "TransparentColorMode": "Clear",
    "NearLossless": false,
    "NearLosslessQuality": 0
  }
}
